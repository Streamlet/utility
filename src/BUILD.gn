group("src") {
  deps = [
    "crypto",
    "log",
    "process",
    "string",
  ]
}

executable("unittest") {
  testonly = true
  if (is_win) {
    configs += [ "../build/config/win:console_subsystem" ]
  }
  sources = [ "unittest.cc" ]
  deps = [
    "../thirdparty:googletest",
    "crypto:unittest",
    "process:unittest",
  ]
}
